{
  "name": "@worldcoin/next-15-template",
  "version": "0.1.0",
  "homepage": "https://docs.worldcoin.org/mini-apps",
  "description": "Create a Mini App",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:seed": "tsx prisma/seed.ts",
    "db:generate": "dotenv -e .env.local -e .env -- prisma generate",
    "db:migrate": "dotenv -e .env.local -e .env -- prisma migrate dev",
    "db:migrate:deploy": "dotenv -e .env.local -e .env -- prisma migrate deploy",
    "db:reset": "dotenv -e .env.local -e .env -- prisma migrate reset",
    "db:push": "dotenv -e .env.local -e .env -- prisma db push",
    "db:studio": "dotenv -e .env.local -e .env -- prisma studio"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.19.2",
    "@worldcoin/mini-apps-ui-kit-react": "^1.0.2",
    "@worldcoin/minikit-js": "latest",
    "@worldcoin/minikit-react": "latest",
    "clsx": "^2.1.1",
    "iconoir-react": "^7.11.0",
    "next": "^15.2.8",
    "next-auth": "^5.0.0-beta.25",
    "react": "^19.0.3",
    "react-dom": "^19.0.3",
    "tailwind-merge": "^3.2.0",
    "viem": "2.23.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^7.4.4",
    "eruda": "^3.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "eslint-plugin-react": "^7.37.5",
    "prisma": "^6.19.2",
    "tailwindcss": "^4",
    "tsx": "^4.7.0",
    "typescript": "^5"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
